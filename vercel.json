{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/services",
      "dest": "/services.html"
    },
    {
      "src": "/expertise",
      "dest": "/expertise.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/contacts",
      "dest": "/contacts.html"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    }
  ]
}